    <Panel>

        <TabControl
            BorderBrush="{DynamicResource MainBorderColor}"
            BorderThickness="0"
            HorizontalAlignment="Left"
            Margin="0,0,0,35"
            Padding="0"
            TabStripPlacement="Left"
            VerticalAlignment="Top"
            x:Name="MainTabControl">
            <TabControl.Styles>
                <Styles>
                    <Style Selector="TabItem.headerStyle">
                        <Setter Property="HorizontalContentAlignment" Value="Left" />
                        <Setter Property="Padding" Value="12, 15" />
                        <Setter Property="Width" Value="250" />
                    </Style>
                    <Style Selector="Image.imageStyle">
                        <Setter Property="Margin" Value="2,0,8,0" />
                        <Setter Property="Width" Value="14" />
                        <Setter Property="Height" Value="14" />
                    </Style>
                    <!-- <Style Selector="Path.pathStyle"> -->
                    <!--     <Setter Property="Margin" Value="2,0,8,0" /> -->
                    <!--     <Setter Property="Width" Value="14" /> -->
                    <!--     <Setter Property="Height" Value="14" /> -->
                    <!--     <Setter Property="Stretch" Value="Fill" /> -->
                    <!--     <Setter Property="Fill" Value="{DynamicResource MainTextColor}" /> -->
                    <!-- </Style> -->
                </Styles>
            </TabControl.Styles>

            <!--  General Settings  -->
            <TabItem Classes="headerStyle" x:Name="GeneralSettingsTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource GeneralSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{Binding GeneralSettings, Mode=OneWay}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:GeneralSettingsView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Appearance Settings  -->
            <TabItem Classes="headerStyle" x:Name="AppearanceTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource AppearanceSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{Binding Appearance, Mode=OneWay}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:AppearanceView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Image Settings  -->
            <TabItem Classes="headerStyle" x:Name="ImageTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource ImageSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{Binding ImageTxt, Mode=OneWay}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:ImageSettingsView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Gallery Settings  -->
            <TabItem Classes="headerStyle" x:Name="GalleryTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource GallerySettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding GallerySettings,
                                                   Mode=OneWay}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:GalleryView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Slideshow Settings  -->
            <TabItem Classes="headerStyle" x:Name="SlideshowTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource SlideshowSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding Slideshow,
                                                   Mode=OneWay}" />
                    </StackPanel>
                </TabItem.Header>
                <ScrollViewer>
                    <views:SlideshowSettingsView Background="Transparent" Margin="20,5,40,20" />
                </ScrollViewer>
            </TabItem>

            <!--  Navigation Settings  -->
            <TabItem Classes="headerStyle" x:Name="NavigationTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource NavigationSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding Navigation}" />
                    </StackPanel>
                </TabItem.Header>

                <customControls:AutoScrollViewer>
                    <views:NavigationView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Zoom Settings  -->
            <TabItem Classes="headerStyle">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource ZoomSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding Zoom}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:ZoomSettingsView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Mouse Wheel Settings  -->
            <TabItem Classes="headerStyle" x:Name="MouseWheelTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource MouseWheelSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding MouseWheel}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:MouseWheelView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Language Settings  -->
            <TabItem Classes="headerStyle" x:Name="LanguageTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource LanguageSettingsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding Language}" />
                    </StackPanel>
                </TabItem.Header>
                <customControls:AutoScrollViewer>
                    <views:LanguageView Background="Transparent" Margin="20,5,40,20" />
                </customControls:AutoScrollViewer>
            </TabItem>

            <!--  Application Shortcuts  -->
            <TabItem Classes="headerStyle" x:Name="AppShortcutsTabItem">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource KeyboardImage}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding ApplicationShortcuts}" />
                    </StackPanel>
                </TabItem.Header>
                <Button
                    Background="{DynamicResource BackgroundAlpha}"
                    BorderBrush="{DynamicResource SecondaryBorderColor}"
                    Classes="ButtonBorder altHover"
                    Command="{CompiledBinding ShowKeybindingsWindowCommand}"
                    HorizontalAlignment="Left"
                    Margin="20,40,00,20"
                    Padding="0,0,7,0"
                    ToolTip.Tip="{CompiledBinding ApplicationShortcuts,
                                                  Mode=OneWay}"
                    VerticalAlignment="Top"
                    Width="NaN">
                    <StackPanel Orientation="Horizontal">
                        <Image
                            Height="17"
                            Margin="9,0,12,0"
                            Source="{StaticResource KeyboardImage}"
                            Width="17" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="{CompiledBinding ApplicationShortcuts,
                                                   Mode=OneWay}" />
                    </StackPanel>
                </Button>
            </TabItem>

            <!--  File associations  -->
            <TabItem
                Classes="headerStyle"
                IsEnabled="False"
                x:Name="FileAssociationsTabItem ">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Classes="imageStyle" Source="{StaticResource FileActionsIcon}" />
                        <TextBlock
                            Classes="txt"
                            Foreground="{StaticResource SecondaryTextColor}"
                            Text="File associations" />
                    </StackPanel>
                </TabItem.Header>
            </TabItem>
        </TabControl>


        <Button
            BorderThickness="0,1,1,0"
            Classes="BorderStyle altHover mainBtn"
