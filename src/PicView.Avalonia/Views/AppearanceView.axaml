<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="PicView.Avalonia.Views.AppearanceView"
    x:DataType="viewModels:MainViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:PicView.Avalonia.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.DataContext>
        <viewModels:MainViewModel />
    </Design.DataContext>
    <StackPanel HorizontalAlignment="Center" Orientation="Vertical">

        <TextBlock
            Classes="txt"
            FontFamily="/Assets/Fonts/Roboto-Bold.ttf#Roboto"
            FontSize="14"
            Foreground="{StaticResource SecondaryTextColor}"
            Margin="0,20,0,20"
            Text="{CompiledBinding Theme,
                                   Mode=OneWay}" />

        <ComboBox
            Background="{DynamicResource SecondaryBackgroundColor}"
            BorderBrush="{DynamicResource MainBorderColor}"
            BorderThickness="1"
            FontFamily="/Assets/Fonts/Roboto-Medium.ttf#Roboto"
            Foreground="{StaticResource SecondaryTextColor}"
            Margin="0,0,0,10"
            Padding="5,7,0,7"
            Width="300"
            x:Name="ThemeBox">
            <ComboBoxItem Content="{CompiledBinding DarkTheme, Mode=OneWay}" x:Name="DarkThemeBox" />
            <ComboBoxItem Content="{CompiledBinding LightTheme, Mode=OneWay}" x:Name="LightThemeBox" />
            <ComboBoxItem Content="{CompiledBinding GlassTheme, Mode=OneWay}" x:Name="GlassThemeBox" />
        </ComboBox>

        <Button
            Classes="altHover"
            Command="{Binding RestartCommand}"
            Margin="0,0,0,20">
            <TextBlock
                Classes="txt"
                FontFamily="/Assets/Fonts/Roboto-Bold.ttf#Roboto"
                Foreground="{StaticResource SecondaryTextColor}"
                Padding="5"
                Text="{CompiledBinding ChangingThemeRequiresRestart,
                                       Mode=OneWay}" />
        </Button>

        <TextBlock
            Classes="txt"
            FontFamily="/Assets/Fonts/Roboto-Bold.ttf#Roboto"
            FontSize="14"
            Foreground="{StaticResource SecondaryTextColor}"
            Margin="0,5,0,20"
            Text="{CompiledBinding HighlightColor,
                                   Mode=OneWay}" />

        <WrapPanel
            HorizontalAlignment="Center"
            Margin="0,0,0,10"
            MaxWidth="300"
            Orientation="Horizontal">
            <WrapPanel.Styles>
                <Styles>
                    <Style Selector="Button.colorBtn">
                        <Setter Property="CornerRadius" Value="6" />
                        <Setter Property="Margin" Value="5" />
                        <Setter Property="Width" Value="35" />
                        <Setter Property="Height" Value="35" />
                    </Style>
                    <Style Selector="Button.colorBtn.active">
                        <Setter Property="BorderThickness" Value="2" />
                        <Setter Property="BorderBrush" Value="#FFFFFF" />
                    </Style>
                    <Style Selector="Button.colorBtn:pointerover">
                        <Setter Property="BorderThickness" Value="2" />
                        <Setter Property="BorderBrush" Value="#FFFFFF" />
                    </Style>
                </Styles>
            </WrapPanel.Styles>

            <!--  Blue  -->
            <Button
                Background="#1A8CF0"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="BlueButton" />
            <!--  Cyan  -->
            <Button
                Background="#1BA1E2"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="CyanButton" />
            <!--  Aqua  -->
            <Button
                Background="#36E6CC"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="AquaButton" />
            <!--  Teal  -->
            <Button
                Background="#44A1A0"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="TealButton" />
            <!--  Lime  -->
            <Button
                Background="#20E76B"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="LimeButton" />
            <!--  Green  -->
            <Button
                Background="#22CB97"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="GreenButton" />
            <!--  Gold  -->
            <Button
                Background="#FEA955"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="GoldButton" />
            <!--  Orange  -->
            <Button
                Background="#DB5B3D"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="OrangeButton" />
            <!--  Red  -->
            <Button
                Background="#F91110"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="RedButton" />
            <!--  Pink  -->
            <Button
                Background="#FF35C5"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="PinkButton" />
            <!--  Magenta  -->
            <Button
                Background="#E68BEE"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="MagentaButton" />
            <!--  Purple  -->
            <Button
                Background="#9738EB"
                Classes="colorBtn"
                Click="ColorButton_OnClick"
                x:Name="PurpleButton" />
        </WrapPanel>


        <TextBlock
            Classes="txt"
            FontFamily="/Assets/Fonts/Roboto-Bold.ttf#Roboto"
            FontSize="14"
            Foreground="{StaticResource SecondaryTextColor}"
            Margin="0,20,0,10"
            Text="{CompiledBinding MiscSettings,
                                   Mode=OneWay}" />


