    <Panel x:Name="MainGrid">
        <uc:SpinWaiter
            HorizontalAlignment="Center"
            IsVisible="{CompiledBinding IsLoading,
                                        Mode=OneWay}"
            VerticalAlignment="Center"
            ZIndex="3" />
