                </MenuItem>
            </MenuItem>
            <MenuItem
                Command="{CompiledBinding CopyFileCommand}"
                CommandParameter="{CompiledBinding FileInfo.FullName,
                                                   FallbackValue=''}"
                Header="{CompiledBinding CopyFile,
                                         Mode=OneWay}">
                <MenuItem.Icon>
                    <Path
                        Data="{StaticResource CopyGeometry}"
                        Fill="{StaticResource Brush0}"
                        Height="12"
                        Stretch="Fill"
                        Width="12" />
                </MenuItem.Icon>
            </MenuItem>
