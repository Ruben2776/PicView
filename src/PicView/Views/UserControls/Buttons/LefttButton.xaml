<UserControl
    x:Class="PicView.Views.UserControls.Buttons.LeftButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Canvas
        x:Name="TheButton"
        Width="90"
        Height="25"
        FocusVisualStyle="{x:Null}"
        Focusable="False">
        <Canvas.Background>
            <SolidColorBrush x:Name="LeftButtonBrush" Color="Transparent" />
        </Canvas.Background>
        <Path
            Canvas.Left="35.166"
            Canvas.Top="1"
            Width="19.669"
            Height="22.62"
            Data="F1M27.008,0L27.01,31.062 0,15.529z"
            Stretch="Fill">
            <Path.Fill>
                <SolidColorBrush x:Name="LeftArrowFill" Color="{StaticResource IconColor}" />
            </Path.Fill>
        </Path>
    </Canvas>
</UserControl>