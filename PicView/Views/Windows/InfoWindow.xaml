                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,1">
                        <StackPanel
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                            <TextBlock FontSize="12" Text="{StaticResource ResourceKey=Shift}" />
                            <TextBlock
                                FontSize="12"
                                Foreground="{StaticResource MainColorFadedBrush}"
                                Text=" + " />
                            <TextBlock FontSize="12" Text="{StaticResource ResourceKey=DoubleClick}" />
                        </StackPanel>
                    </Border>

                    <Border
                        Grid.Row="7"
                        Grid.Column="2"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,1" />

                    <Border
                        Grid.Row="8"
                        Grid.Column="0"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="1,1,1,1">
                        <TextBlock
                            Margin="10,3,0,3"
                            FontSize="12"
                            Text="{StaticResource ResourceKey=NewWindow}" />
                    </Border>

                    <Border
                        Grid.Row="8"
                        Grid.Column="1"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,1">
                        <StackPanel
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                            <TextBlock FontSize="12" Text="{StaticResource ResourceKey=Ctrl}" />
                            <TextBlock
                                FontSize="12"
                                Foreground="{StaticResource MainColorFadedBrush}"
                                Text=" + " />
                            <TextBlock FontSize="12" Text="N" />
                        </StackPanel>
                    </Border>

                    <Border
                        Grid.Row="8"
                        Grid.Column="2"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,1" />

                    <Border
                        Grid.Row="9"
                        Grid.Column="0"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="1,1,1,1">
                        <TextBlock
                            Margin="10,3,0,3"
                            FontSize="12"
                            Text="{StaticResource ResourceKey=StayTopMost}" />
                    </Border>

                    <Border
                        Grid.Row="9"
                        Grid.Column="1"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,1">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="12"
                            Text="T" />
                    </Border>

                    <Border
                        Grid.Row="9"
                        Grid.Column="2"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,1" />
                </Grid>

                <Label
                    Width="480"
                    Margin="-4,0,0,10"
                    d:Content="Additional functions"
                    Content="{StaticResource ResourceKey=AdditionalFunctions}" />

                <Grid Width="480" Margin="0,0,0,16">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="130" />
                        <ColumnDefinition Width="170" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <Border
                        Grid.Row="0"
                        Grid.Column="0"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="1,1,1,0">
                        <TextBlock
                            Margin="10,3,0,3"
                            d:Text="Open with.."
                            FontSize="12"
                            Text="{StaticResource ResourceKey=OpenWith}" />
                    </Border>

                    <Border
                        Grid.Row="0"
                        Grid.Column="1"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,0">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="12"
                            Text="E" />
                    </Border>

                    <Border
                        Grid.Row="0"
                        Grid.Column="2"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,0" />

                    <Border
                        Grid.Row="1"
                        Grid.Column="0"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="1,1,1,0">
                        <TextBlock
                            Margin="10,3,0,3"
                            d:Text="Reload"
                            FontSize="12"
                            Text="{StaticResource ResourceKey=Reload}" />
                    </Border>

                    <Border
                        Grid.Row="1"
                        Grid.Column="1"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,0">
                        <StackPanel
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                            <TextBlock FontSize="12" Text="{StaticResource ResourceKey=Ctrl}" />
                            <TextBlock
                                FontSize="12"
                                Foreground="{StaticResource MainColorFadedBrush}"
                                Text=" + " />
                            <TextBlock FontSize="12" Text="R" />
                        </StackPanel>
                    </Border>

                    <Border
                        Grid.Row="1"
                        Grid.Column="2"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundColorBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,1,1,0" />

                    <Border
                        Grid.Row="2"
                        Grid.Column="0"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="1,0,1,1">
                        <TextBlock
                            Margin="10,3,0,3"
                            FontSize="12"
                            Text="{StaticResource ResourceKey=ShowHorizontalGallery}" />
                    </Border>

                    <Border
                        Grid.Row="2"
                        Grid.Column="1"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,0,1,1">
                        <TextBlock
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="12"
                            Text="G" />
                    </Border>

                    <Border
                        Grid.Row="2"
                        Grid.Column="2"
                        Padding="0,3,0,3"
                        Background="{StaticResource BackgroundSubtleHighlightBrush}"
                        BorderBrush="{StaticResource BorderBrush}"
                        BorderThickness="0,0,1,1" />
                </Grid>

                <StackPanel Margin="0,10,0,15">

                    <Label
                        HorizontalAlignment="Center"
                        Content="{StaticResource ResourceKey=Credits}"
                        FontFamily="/PicView;component/Themes/Resources/fonts/#Roboto Black"
                        FontSize="16" />

                    <Label
                        Margin="0,0,0,3"
                        HorizontalAlignment="Center"
                        Content="{StaticResource ResourceKey=IconsUsed}"
                        FontFamily="/PicView;component/Themes/Resources/fonts/#Roboto Black" />

                    <TextBlock
                        Margin="0,0,0,2"
                        FontSize="14"
                        TextAlignment="Center"
                        ToolTip="https://useiconic.com/open/">
                        <Hyperlink
                            x:Name="Iconic"
                            Focusable="False"
                            NavigateUri="https://useiconic.com/open/"
                            RequestNavigate="Hyperlink_RequestNavigate">
                            Iconic
                            <Hyperlink.Foreground>
                                <SolidColorBrush x:Name="IconicBrush" Color="{StaticResource MainColor}" />
                            </Hyperlink.Foreground>
                        </Hyperlink>
                    </TextBlock>

                    <TextBlock
                        x:Name="Ionic"
                        Margin="0,0,0,2"
                        FontSize="14"
                        TextAlignment="Center"
                        ToolTip="http://Ionicons.com/">
                        <Hyperlink NavigateUri="http://Ionicons.com/" RequestNavigate="Hyperlink_RequestNavigate">
                            Ioinicons
                            <Hyperlink.Foreground>
                                <SolidColorBrush x:Name="IonicBrush" Color="{StaticResource MainColor}" />
                            </Hyperlink.Foreground>
                        </Hyperlink>
                    </TextBlock>

                    <TextBlock TextAlignment="Center" ToolTip="http://fontawesome.io/">
                        <Hyperlink NavigateUri="http://fontawesome.io/" RequestNavigate="Hyperlink_RequestNavigate">
                            <TextBlock
                                x:Name="FontAwesome"
                                Margin="0,0,0,2"
                                FontSize="14"
                                Text="FontAwesome" />
                            <Hyperlink.Foreground>
                                <SolidColorBrush x:Name="FontAwesomeBrush" Color="{StaticResource MainColor}" />
                            </Hyperlink.Foreground>
                        </Hyperlink>
                    </TextBlock>

                    <TextBlock TextAlignment="Center">
                        <Hyperlink NavigateUri="http://www.zondicons.com/icons.html" RequestNavigate="Hyperlink_RequestNavigate">
                            <TextBlock
                                x:Name="zondicons"
                                Margin="0,0,0,2"
                                FontSize="14"
                                Text="Zondicons"
                                ToolTip="http://www.zondicons.com/icons.html" />
                            <Hyperlink.Foreground>
                                <SolidColorBrush x:Name="zondiconsBrush" Color="{StaticResource MainColor}" />
                            </Hyperlink.Foreground>
                        </Hyperlink>
                    </TextBlock>

                    <TextBlock TextAlignment="Center">
                        <Hyperlink NavigateUri="https://www.flaticon.com/authors/freepik" RequestNavigate="Hyperlink_RequestNavigate">
                            <TextBlock
                                x:Name="freepik"
                                Margin="0,0,0,2"
                                FontSize="14"
                                Text="Freepik"
                                ToolTip="https://www.flaticon.com/authors/freepik" />
                            <Hyperlink.Foreground>
                                <SolidColorBrush x:Name="FreepikBrush" Color="{StaticResource MainColor}" />
                            </Hyperlink.Foreground>
                        </Hyperlink>
                    </TextBlock>
                </StackPanel>
            </StackPanel>
        </ScrollViewer>
    </DockPanel>
</Window>